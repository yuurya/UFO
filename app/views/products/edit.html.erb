<% if user_signed_in? %>

  <h1>編集画面</h1>
  <!-- form_with部分 -->
  <%= form_with model:@product, local: true do |f| %>
    <h4>タイトル</h4>
    <%= f.text_field :title %>
    <h4>商品名</h4>
    <%= f.text_field :product_name%>
    <h4>本文</h4>
    <%= f.text_area :body %>
    <%= f.submit '保存' %>
  <% end %>
<% end %>